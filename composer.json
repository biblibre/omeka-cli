{
    "name": "biblibre/omeka-cli",
    "description": "Omeka CLI",
    "type": "project",
    "keywords": ["omeka"],
    "homepage": "https://github.com/biblibre/omeka-cli",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Julian Maurice",
            "email": "julian.maurice@biblibre.com"
        }
    ],
    "support": {
        "issues": "https://github.com/biblibre/omeka-cli/issues"
    },
    "require": {
        "ext-zip": "*",
        "knplabs/github-api": "^2.0",
        "php-http/guzzle6-adapter": "^1.1",
        "psr/log": "^1.0",
        "fabpot/goutte": "^3.2",
        "corneltek/getoptionkit": "^2.5",
        "cache/filesystem-adapter": "^1.0",
        "jeremeamia/SuperClosure": "^2.3",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "OmekaCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OmekaCli\\Test\\": "tests/OmekaCli/Test/"
        }
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "bin": ["bin/omeka-cli"]
}
