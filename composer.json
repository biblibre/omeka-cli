{
    "name": "biblibre/omeka-cli",
    "description": "Omeka CLI",
    "type": "project",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Julian Maurice",
            "email": "julian.maurice@biblibre.com"
        }
    ],
    "autoload": {
        "psr-4": {
